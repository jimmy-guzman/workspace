  "scripts": {
    "start:stories": "start-storybook -p 9001 -c .storybook",
    "build:stories": "build-storybook -c .storybook -o .storybook/dist/",
    "start:app:portfolio": "yarn workspace @jimmy-guzman/portfolio start",
    "build:app:portfolio": "yarn workspace @jimmy-guzman/portfolio build",
    "lint": "eslint --cache --cache-location ./.cache/eslint/ --format codeframe '**/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn --silent lint --fix",
    "prettify": "prettier '**/**/*.{js,jsx,ts,tsx,md}' --write",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "affected:test": "lerna run --since origin/develop test",
    "build": "lerna run --stream build",
    "affected:build": "lerna run --since origin/develop build",
    "cz": "git-cz",
    "release:npm": "cross-env HUSKY_BYPASS=true lerna publish --message 'chore(release): ðŸ¤– npm publish'",
    "prepare": "lerna run --stream prepare"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lerna run --concurrency 1 --stream precommit",
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    }
  },
